{
  
  "apps" : [{
    "name": "hr-manage",
    "script": "server.js",
    "instances": 2,
    "autorestart": true,
    "watch": true,
    "max_memory_restart": "500M",
    "interpreter": "node_modules/babel-cli/bin/babel-node.js",
    "err_file": "logs/err.log",
    "out_file": "logs/out.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "env": {
      "NODE_ENV": "production"
    },
    "env_dev": {
      "NODE_ENV": "development"
    },
    "env_pred": {
      "NODE_ENV": "pred"
    },
    "env_prod": {
      "NODE_ENV": "production"
    }
  }]
}